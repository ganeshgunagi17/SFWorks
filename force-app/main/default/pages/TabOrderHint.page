<apex:page standardController="Account">
    <apex:form >
    	<apex:pageBlock >
        	<apex:pageBlockSection >
            	<apex:inputField value="{!Account.Name}" taborderhint="4"/>
                <apex:inputField value="{!Account.Industry}" taborderhint="3"/>
                <apex:inputField value="{!Account.Rating}" taborderhint="2"/>
                <apex:inputField value="{!Account.Phone}" taborderhint="1"/>
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
               
</apex:page>