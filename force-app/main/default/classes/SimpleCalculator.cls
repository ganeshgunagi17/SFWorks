public class SimpleCalculator {
    public Integer num1{get;set;}
    public Integer num2{get;set;}
    public Integer result{get;set;}
    
    public void add(){
        result=num1+num2;
    }
    public void sub(){
        result=num1-num2;
    }
public void mul(){
        result=num1*num2;
    }
    public void div(){
        result=num1-num2;
    }
}